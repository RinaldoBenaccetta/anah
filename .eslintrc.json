{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": [
        "standard"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-prototype-builtins": "off",
        "max-len": [
            "error",
            {
                "code": 80,
                "ignoreStrings": true,
                "ignoreUrls": true
            }
        ]
    }
}
